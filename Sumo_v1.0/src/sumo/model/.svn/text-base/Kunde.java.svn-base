package sumo.model;

import java.sql.Date;

/*
 * Ersteller: Daniel Burgstaller
 * Klassenname: Kunde
 * Superklasse: - 
 * Interfaces: -
 * Funktionen: -
 */
public class Kunde {
	private long id;
	private String name;
	private String anschrift;
	private short plz;
	private String ort;
	private float guthaben;
	private String status;
	private Date letzterBesuch;
	private String username;
	private String password;
	private String email;
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAnschrift() {
		return anschrift;
	}
	public void setAnschrift(String anschrift) {
		this.anschrift = anschrift;
	}
	public short getPlz() {
		return plz;
	}
	public void setPlz(short plz) {
		this.plz = plz;
	}
	public String getOrt() {
		return ort;
	}
	public void setOrt(String ort) {
		this.ort = ort;
	}
	public float getGuthaben() {
		return guthaben;
	}
	public void setGuthaben(float guthaben) {
		this.guthaben = guthaben;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Date getLetzterBesuch() {
		return letzterBesuch;
	}
	public void setLetzterBesuch(Date letzterBesuch) {
		this.letzterBesuch = letzterBesuch;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
}//#Kunde
