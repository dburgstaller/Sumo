package sumo.model;

import java.sql.Date;

public class Mitarbeiter {
	private Long id;
	private String name;
	private String anschrift;
	private short plz;
	private String ort;
	private float gehalt;
	private Date einstiegsdatum;
	private long kontoNr;
	private short blz;
	private String username;
	private String passwort;
	private String bemerkung;
	private boolean isAdmin;
	private String bildpfad;
	
	
	public String getBildpfad() {
		return bildpfad;
	}
	public void setBildpfad(String bildpfad) {
		this.bildpfad = bildpfad;
	}
	public void setAdmin(boolean isAdmin) {
		this.isAdmin = isAdmin;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAnschrift() {
		return anschrift;
	}
	public void setAnschrift(String anschrift) {
		this.anschrift = anschrift;
	}
	public short getPlz() {
		return plz;
	}
	public void setPlz(short plz) {
		this.plz = plz;
	}
	public String getOrt() {
		return ort;
	}
	public void setOrt(String ort) {
		this.ort = ort;
	}
	public float getGehalt() {
		return gehalt;
	}
	public void setGehalt(float gehalt) {
		this.gehalt = gehalt;
	}
	public Date getEinstiegsdatum() {
		return einstiegsdatum;
	}
	public void setEinstiegsdatum(Date einstiegsdatum) {
		this.einstiegsdatum = einstiegsdatum;
	}
	public long getKontoNr() {
		return kontoNr;
	}
	public void setKontoNr(long kontoNr) {
		this.kontoNr = kontoNr;
	}
	public short getBlz() {
		return blz;
	}
	public void setBlz(short blz) {
		this.blz = blz;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPasswort() {
		return passwort;
	}
	public void setPasswort(String passwort) {
		this.passwort = passwort;
	}
	public String getBemerkung() {
		return bemerkung;
	}
	public void setBemerkung(String bemerkung) {
		this.bemerkung = bemerkung;
	}
	public boolean getIsAdmin() {
		return isAdmin;
	}
	public void setIsAdmin(boolean isAdmin) {
		this.isAdmin = isAdmin;
	}
	
	
}
